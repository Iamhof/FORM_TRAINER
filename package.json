    "cross-env": "^7.0.3",
    "eslint": "9.31.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.10",
    "typescript": "~5.9.2",
    "vitest": "^2.1.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --ext ts,tsx --max-warnings=0"
    ]
  },
  "private": true
