{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start --tunnel",
    "dev:web": "expo start --web --tunnel",
    "dev:web:debug": "cross-env DEBUG=expo* expo start --web --tunnel",
    "start": "bun run dev",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint . --ext ts,tsx --max-warnings=0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "ci": "bun run lint && bun run typecheck && bun run test",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.86",
    "@expo/vector-icons": "^15.0.3",
    "@hono/trpc-server": "^0.4.0",
